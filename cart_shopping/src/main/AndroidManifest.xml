            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.CarActivity" />
        <activity android:name=".View.OrderActivity" />
        <activity android:name=".View.XiaDanActivity"></activity>
    </application>

</manifest>